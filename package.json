{
  "name": "release-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager ./ --overwrite --platform=darwin --arch=x64 --icon=logo.png --out=build",
    "package-win64": "electron-packager ./ rrtracker --overwrite --platform=win32 --arch=x64 --icon=logo.ico --out=build --version-string.CompanyName=RampantEpsilon --version-string.ProductName=\"Rampant Release Tracker\"",
    "package-win32": "electron-packager ./ rrtracker --overwrite --platform=win32 --arch=ia32 --icon=logo.ico --out=build --version-string.CompanyName=RampantEpsilon --version-string.ProductName=\"Rampant Release Tracker\"",
    "package-linux": "electron-packager ./ rrtracker --platform=linux --arch=x64 --overwrite --icon=logo.png --out=build"
  },
  "author": "Tom \"RampantEpsilon\" Ware",
  "license": "ISC",
  "dependencies": {
    "electron": "^12.0.4",
    "jquery": "^3.6.0"
  }
}
